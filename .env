# History Scanner App - Environment Variables
# Replace these values with your actual API keys and configuration

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ElevenLabs API Configuration (for voice synthesis)
EXPO_PUBLIC_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# App Configuration
EXPO_PUBLIC_APP_SCHEME=historyscanner
EXPO_PUBLIC_SITE_URL=https://rork.com
EXPO_PUBLIC_REDIRECT_URL=historyscanner://email-confirmation

# Development/Production Environment
NODE_ENV=development

# Image Upload Configuration
EXPO_PUBLIC_MAX_IMAGE_SIZE=5242880

# Voice Service Configuration
EXPO_PUBLIC_VOICE_ENABLED=true

# Security Configuration
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_SESSION_TIMEOUT=60
