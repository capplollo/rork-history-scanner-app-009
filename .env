# Environment Variables
# Monument Scanner App Configuration

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_actual_anon_key_here

# Rork API Configuration
EXPO_PUBLIC_RORK_API_BASE_URL=https://toolkit.rork.com

# IMPORTANT: DO NOT PUT API KEYS HERE - THEY WILL BE LEAKED TO GITHUB
# The Rork toolkit handles AI requests without needing API keys
# OPENAI_API_KEY=your_openai_api_key_here

# ElevenLabs API Configuration (for voice synthesis)
EXPO_PUBLIC_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# App Configuration
EXPO_PUBLIC_APP_SCHEME=monumentscanner
EXPO_PUBLIC_SITE_URL=https://rork.com
EXPO_PUBLIC_REDIRECT_URL=monumentscanner://email-confirmation

# Development/Production Environment
NODE_ENV=development

# Image Upload Configuration
EXPO_PUBLIC_MAX_IMAGE_SIZE=5242880

# Voice Service Configuration
EXPO_PUBLIC_VOICE_ENABLED=true

# Security Configuration
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_SESSION_TIMEOUT=60
